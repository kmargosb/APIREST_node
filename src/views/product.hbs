{{#each rows}}
<div class="card mb-3">
  <img src="{{imageURL}}" class="card-img-top" alt="..." style="width: 68rem; max-height: 38.125rem; object-fit: cover;">
  <div class="card-body">
    <h5 class="card-title">{{title}}</h5>
    <p class="card-text">{{descrip}}</p>
    <p class="card-text"><small class="text-body-secondary">Last updated 3 mins ago</small></p>
  </div>
</div>
{{/each}}